{
  "npmScope": "leap",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "portal": {
      "tags": []
    },
    "portal-e2e": {
      "tags": [],
      "implicitDependencies": ["portal"]
    },
    "uikit": {
      "tags": []
    },
    "uikit-e2e": {
      "tags": [],
      "implicitDependencies": ["uikit"]
    },
    "store": {
      "tags": []
    }
  }
}
